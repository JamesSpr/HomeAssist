{
    "compilerOptions": {
      "target": "es5",
      "outDir": "./dist/",
      "lib": [
        "dom",
        "dom.iterable",
        "esnext"
      ],
      "allowJs": true,
      "jsx": "react",
      "moduleResolution": "node",
      "strict": true,
      "sourceMap": true,
      "esModuleInterop": true,
      "isolatedModules": true,
      "allowSyntheticDefaultImports": true,
    },
    "include": ["src/**/*"],
    "exclude": [
      "node_modules"
    ]
}
